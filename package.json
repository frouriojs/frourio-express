{
  "name": "frourio-express",
  "version": "0.0.0",
  "description": "Fast and type-safe full stack framework, for TypeScript",
  "author": "Solufa <solufa2020@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "bin/index.js",
  "homepage": "https://frourio.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frouriojs/frourio-express.git"
  },
  "bugs": {
    "url": "https://github.com/frouriojs/frourio-express/issues"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "aspida",
    "express"
  ],
  "scripts": {
    "dev": "npm run build && cd servers && aspida && node build.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "eslint . && prettier --check \"./**/*.{ts,js}\"",
    "lint:fix": "eslint . --fix && prettier --write \"./**/*.{ts,js}\"",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/multer": "^1.4.7",
    "aspida": "^1.13.3",
    "fast-json-stringify": "^5.8.0",
    "multer": "^1.4.5-lts.1",
    "velona": "^0.8.0"
  },
  "devDependencies": {
    "@aspida/axios": "^1.13.3",
    "@aspida/node-fetch": "^1.13.3",
    "@types/express": "^4.17.17",
    "@types/node-fetch": "^2.6.4",
    "axios": "^1.4.0",
    "eslint": "^8.46.0",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.2.3",
    "express": "^4.18.2",
    "node-fetch": "^2.6.12",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "typescript-eslint": "^7.16.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.3",
    "zod": "^3.21.4"
  }
}
